{
  "name": "waic-calculator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "oxlint --fix",
    "format": "prettier . --write --experimental-cli",
    "typecheck": "tsc -b",
    "check": "bun lint && bun format && bun typecheck"
  },
  "devDependencies": {
    "@prettier/plugin-oxc": "^0.1.3",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "clsx": "^2.1.1",
    "neverthrow": "^8.2.0",
    "oxlint": "^1.43.0",
    "prettier": "3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "8.0.0-beta.11",
    "zustand": "^5.0.11"
  },
  "overrides": {
    "vite": "8.0.0-beta.11"
  },
  "prettier": {
    "plugins": [
      "@prettier/plugin-oxc",
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ],
    "printWidth": 120
  }
}